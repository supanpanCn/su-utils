{
  "name": "su-helpers",
  "version": "0.0.50",
  "description": "",
  "scripts": {
    "preinstall": "npx npm-only-allow@latest --PM yarn --lang zh",
    "build": "tsup src/*.ts --format cjs,esm --dts --splitting --clean --watch src",
    "dev": "node ./dist/index"
  },
  "files": [
    "dist"
  ],
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*"
      ]
    }
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./node": {
      "types": "./dist/node.d.ts",
      "require": "./dist/node.js",
      "import": "./dist/node.mjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/supanpanCn/su-utils.git"
  },
  "keywords": [],
  "author": "supanpan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/supanpanCn/su-utils/issues"
  },
  "homepage": "https://github.com/supanpanCn/su-utils#readme",
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@types/prettier": "^2.7.2",
    "prettier": "^2.8.8",
    "tsup": "^6.5.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "displace-comments": "1.0.4",
    "local-pkg": "^0.4.3",
    "picocolors": "^1.0.0",
    "recast": "^0.22.0"
  }
}
